<% if @question.errors.any? %>
  // Render new question form with errors
  $('.new_question_form').html("<%= j render 'surveys/question_form', question: @question, survey: @survey %>");
<% else %>
  $('.new-question-modal').modal('hide');
  $('#tab_content_questions-container').html("<%= j render 'surveys/tab_content_questions', question: @question, survey: @survey %>");
  $("#custom-questions").click();
<% end %>

